.container
  h1 All Seasons
  a(href='/#/board' target="_blank") Player View
  br
  a(href='https://www.myinstants.com/favorites/' target="_blank") Sound Board
  table.table.table-striped.table-hover.table-bordered
    tr
      th Name
      th Description
      th Note
    tr(ng-repeat='season in data')
      td
        a(ui-sref='season({ id: season.id })') {{ season.name }}
      td {{season.description}}
      td {{season.note}}
